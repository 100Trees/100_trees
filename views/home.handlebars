<div id="map"></div>
<script>
  var map;
  function initMap() {
  	if (App.coordinates) {
  		map = new google.maps.Map(document.getElementById('map'), {
	      center: {lat: App.coordinates.latitude, lng: App.coordinates.longitude},
	      zoom: 8
	    });
  	} else {
  		map = new google.maps.Map(document.getElementById('map'), {
	      center: {lat: 41.186, lng: -123.988},
	      zoom: 8
	    });
  	}
  }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key={{key}}&callback=initMap"
async defer></script>